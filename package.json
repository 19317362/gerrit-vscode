{
	"name": "gerrit-vscode",
	"displayName": "gerrit-vscode",
	"description": "A Gerrit extension for vscode",
	"version": "0.0.1",
	"publisher": "tht13",
	"engines": {
		"vscode": "^0.10.8"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.activateGerrit"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.activateGerrit",
			"title": "Open Command Pane",
            "category": "Gerrit"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^1.8.2",
		"vscode": "^0.11.1"
	}
}