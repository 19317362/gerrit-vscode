{
    "name": "gerrit-vscode",
    "displayName": "gerrit-vscode",
    "description": "A Gerrit extension for vscode",
    "version": "0.0.1",
    "publisher": "tht13",
    "engines": {
        "vscode": "^0.10.8"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "gerrit.checkoutBranch",
                "title": "Checkout branch",
                "category": "Gerrit"
            },
            {
                "command": "gerrit.checkoutRevision",
                "title": "Checkout revision",
                "category": "Gerrit"
            },
            {
                "command": "gerrit.cherrypickRevision",
                "title": "Cherrypick revision",
                "category": "Gerrit"
            },
            {
                "command": "gerrit.commit",
                "title": "Commit",
                "category": "Gerrit"
            },
            {
                "command": "gerrit.commitAmend",
                "title": "Commit amend",
                "category": "Gerrit"
            },
            {
                "command": "gerrit.pushBranch",
                "title": "Push to branch",
                "category": "Gerrit"
            },
            {
                "command": "gerrit.rebaseBranch",
                "title": "Rebase from branch",
                "category": "Gerrit"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.2",
        "vscode": "^0.11.1"
    }
}